#include<stdio.h> 
int main(){
    int i,n,x,a[100],c=0;
    printf("\n Enter the size of array:");
    scanf("%d",&n);
    for(i=0;i<n;++i)
    {
        printf("\n Enter the value of a[%d]",i+1 );
        scanf("%d",&a[i]);
    }
    printf("\nEnter the minimum no. of elements to be divided by 7 :");
    scanf("%d",&x);

    for(i=0;i<n;++i){
        if(a[i]%7==0)
            c=c+1;
    }
            if(c>=x){
                printf("\n YES \n");
              
            }
            else
            { 
                printf("\n NO \n");
        
    }
    
    return 0;
}
