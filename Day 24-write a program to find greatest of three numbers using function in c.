\\write a c program to find greatest of three numbers using function in c
#include<stdio.h> 
int biggest(int, int, int );
int main(){
    int a,b,c;
    printf("\nEnter first integer:");
        scanf("%d",&a);
    printf("\nEnter second integer:");
        scanf("%d",&b);
    printf("\nEnter third integer:");
        scanf("%d",&c);

    printf("\nThe biggest number between %d, %d, and %d is %d",a,b,c,biggest(a,b,c));
    return 0;
}
int biggest(int i ,int j,int k)
{
    if(i>j&&i>k)
    {
        return i;
    }
    else
    {
        if(j>k)
            return j;
        else
        return k;
    }

}
